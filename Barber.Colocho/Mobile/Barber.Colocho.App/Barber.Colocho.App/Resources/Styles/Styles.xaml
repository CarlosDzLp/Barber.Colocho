    <Style x:Key="BackgroundPage" TargetType="ContentPage">
        <Setter Property="BackgroundColor" Value="{AppThemeBinding Light={StaticResource ColorWhite}, Dark={StaticResource ColorWhite}}" />
    </Style>
    <Style x:Key="BackgroundPageTab" TargetType="ContentPage">
        <Setter Property="BackgroundColor" Value="{AppThemeBinding Light={StaticResource ColorDimGray}, Dark={StaticResource ColorDimGray}}" />
    </Style>
